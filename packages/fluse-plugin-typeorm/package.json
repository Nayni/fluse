{
  "name": "fluse-plugin-typeorm",
  "description": "Official TypeORM plugin for fluse.",
  "version": "0.4.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Rutger Hendrickx",
    "url": "https://github.com/Nayni"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nayni/fluse.git"
  },
  "keywords": [
    "fluse",
    "plugin",
    "composable",
    "database",
    "db",
    "orm",
    "seeder",
    "fixture",
    "typeorm"
  ],
  "directories": {
