    "fluse-prettier-config": "1.0.1",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sqlite3": "5.0.0",
    "ts-node-dev": "1.0.0-pre.61",
    "typescript": "4.2.3"
  },
  "peerDependencies": {
    "fluse": "^0.7.0",
    "typeorm": "^0.2.25"
