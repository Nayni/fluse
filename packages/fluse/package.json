{
  "name": "fluse",
  "description": "A composable and type-safe fixture builder.",
  "version": "0.7.2",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Rutger Hendrickx",
    "url": "https://github.com/Nayni"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nayni/fluse.git"
  },
  "keywords": [
    "composable",
    "fixture",
    "data-fixture",
    "database",
    "db",
    "orm",
    "seeder"
  ],
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:ci": "jest --ci --collect-coverage --verbose",
    "compile": "tsc",
    "build": "yarn clean && yarn compile",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "lodash": "4.17.20",
    "semver": "7.3.4"
  },
  "devDependencies": {
    "@types/faker": "5.1.6",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/semver": "7.3.4",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "eslint": "7.20.0",
    "eslint-config-fluse": "^1.0.3",
    "faker": "5.4.0",
    "fluse-prettier-config": "1.0.1",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-environment-node": "26.6.2",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.1",
    "typescript": "4.2.3"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --max-warnings=0",
      "prettier --write"
    ]
  }
}
