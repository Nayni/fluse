{
  "name": "fluse",
  "description": "A fluent and type-safe fixture builder.",
  "version": "0.2.0",
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "Rutger Hendrickx",
    "url": "https://github.com/Nayni"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nayni/fluse.git"
  },
  "keywords": [
    "fluent",
    "fixture",
    "data-fixture",
    "database",
    "db",
    "orm",
    "seeder"
  ],
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint . --ext .ts",
    "compile": "tsc",
    "build": "yarn clean && yarn compile",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "lodash": "4.17.20",
    "semver": "7.3.2"
  },
  "devDependencies": {
    "@types/lodash": "4.14.160",
    "@types/semver": "7.3.3",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.7.0",
    "eslint-config-fluse": "1.0.1",
    "fluse-prettier-config": "1.0.1",
    "lint-staged": "10.2.13",
    "prettier": "2.1.1",
    "rimraf": "3.0.2",
    "typescript": "4.0.2"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --max-warnings=0",
      "prettier --write"
    ]
  }
}
